<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dy.bus.mapper.GoodsMapper">
    <select id="selectDangerGoods" resultType="com.dy.bus.entity.Goods">
        select * from bus_goods where number &lt; dangernum order by number
    </select>

</mapper>
